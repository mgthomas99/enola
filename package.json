{
  "name": "enola",
  "version": "0.3.2",
  "description": "Very fast directory deletion",
  "main": "build/index.js",
  "author": "George Thomas",
  "license": "MIT",
  "keywords": [
    "delete",
    "del",
    "fs",
    "nuke",
    "rd",
    "rf",
    "rmdir",
    "rm"
  ],
  "bin": {
    "enola": "build/index.js",
    "nuke": "build/bin/nuke.js"
  },
  "scripts": {
    "build": "gulp \"build\"",
    "clean": "gulp \"clean\"",
    "lint:javascript": "gulp \"lint:javascript\"",
    "lint:typescript": "gulp \"lint:typescript\"",
    "lint": "gulp \"lint\"",
    "prepublish": "npm-run-all \"lint\" \"clean\" \"build\""
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^5.0.4",
    "@types/log4js": "^2.3.5",
    "@types/node": "^10.10.1",
    "@types/yargs": "^12.0.1",
    "del": "^3.0.0",
    "eslint": "^5.6.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^5.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.3",
    "npm-run-all": "^4.1.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "utf-8-validate": "^5.0.1"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "fs-extra": "^7.0.0",
    "log4js": "^3.0.6",
    "yargs": "^12.0.2"
  }
